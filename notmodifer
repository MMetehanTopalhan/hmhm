pragma solidity ^0.7.0;

contract incsetdec{
int public count = 0 ;

address contract_owner;

constructor(){
contract_owner == msg.sender;

}

function Increment() public {
    count +=1;
}
function Decremnet() public {
    count -=1;
}
 function setcount(int value) public {
     require(msg.sender == contract_owner);
     count = value;
 }


}


